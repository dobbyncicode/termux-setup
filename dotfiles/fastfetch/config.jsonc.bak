{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",

  "logo": {
    "type": "small",
    "padding": { "top": 1 }
  },

  "display": {
    "separator": " : "
  },

  "modules": [
    "separator",

    {
      "type": "os",
      "key": " OS"
    },
    {
      "type": "kernel",
      "key": " KERNEL",
      "format": "{1}"
    },
    {
      "type": "uptime",
      "key": "󰔟 UP TIME"
    },
    {
      "type": "shell",
      "key": " SHELL"
    },
    {
      "type": "terminal",
      "key": " TERMINAL"
    },

    "break",

    {
      "type": "cpu",
      "key": " CPU",
      "format": "{1}"
    },
    {
      "type": "gpu",
      "key": "󰢮 GPU"
    },
    {
      "type": "memory",
      "key": "󰍛 MEMORY"
    },
    "separator"
  ]
}
